    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "ahooks": "3.8.4",
    "antd": "5.24.1",
    "clipboard": "2.0.11",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "echarts": "5.6.0",
    "i18next": "24.2.2",
